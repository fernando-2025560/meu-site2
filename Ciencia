  top: 755.90px;         /* distância do topo da tela */
  left: 37.8px;         /* distância da lateral esquerda */
  width: 320px;      /* largura padrão do vídeo (ajuste se desejar) */
  height: 197px;     /* altura proporcional (16:9) */
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}

/* Estilo do iframe do vídeo */
#customVideoBox5 iframe {
  width: 320px;      /* largura fixa do vídeo */
  height: 180px;     /* altura proporcional */
  border: none;      /* remove borda */
}

/* Legenda abaixo do vídeo */
#videoCaption5 {
  margin-top: 3.78px;   /* legenda a 1mm abaixo do vídeo */
  font-size: 13px;
  text-align: absolute;
}

/* Link da legenda */
#videoCaption5 a {
  text-decoration: underline;
  color: #000; /* cor da legenda (ajuste conforme fundo) */
  padding: 3px 6px;
  border-radius: 4px;
}








