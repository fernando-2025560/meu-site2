          <script>
            function buscarNoGoogle() {
              const termo = document.getElementById("campoBusca").value.trim();
              if (termo !== "") {
                const url = "https://www.google.com.br/search?q=" + encodeURIComponent(termo);
                window.open.href = url;
              }
            }
        
            
          </script>
        
        
